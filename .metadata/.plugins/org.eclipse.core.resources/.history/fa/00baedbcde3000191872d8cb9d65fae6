package cn.sxt.oop;

public class TestArraySort {

	public static void main(String[] args){
	//	int[] s=new int[10];
	//	s[1]=3;
		int[] a={3,5,1,6,8,2,4,9,0,7};
		//ц╟ещеепР
		for(int i=0;i<a.length-1;i++){
			if(a[i]>a[i+1]){
				int temp=a[i];
				a[i]=a[i+1];
				a[i+1]=temp;
			}
			
		System.out.println(a[i]);
		}
	}
}
