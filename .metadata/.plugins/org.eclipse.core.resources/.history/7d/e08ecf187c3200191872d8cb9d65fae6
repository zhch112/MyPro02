	public static void main(String[] args){
		File file=new File("F:/¶¯Âþ");
		PrintFile(file);
	}
	public static void PrintFile(File f){
		System.out.println(f.getName());
		if(null==f||!f.exists()){
			return;
		}else if(f.isDirectory()) {
			for(File s:f.listFiles()){
				//System.out.println(s.getName());
				//PrintFile(s);
			}
		}
	}
}
